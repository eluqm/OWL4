			<xs:extension base="AnnotationStatement">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AnnotationOfAnnotationIsComparedWithAnnotationOfAnnotationStatement">
