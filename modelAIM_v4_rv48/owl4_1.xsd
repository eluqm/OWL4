				<xs:sequence>
					<xs:element name="maxOperator" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>An operator represents comparison operators, i.e. &gt;, &gt;=, &lt;, &lt;=,==,!=.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="maxValue" type="xs:string" minOccurs="1" maxOccurs="1">
