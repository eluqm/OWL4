							<xs:documentation>A string representation of UCUM unit for a number in Numerical.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A numeric value of a number in Numerical class.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Interval">
